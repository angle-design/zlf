.nav-tabs  {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 48px;
  background: white;
  display: flex;
  padding-bottom: env(safe-area-inset-bottom);
  border-top:1rpx solid #eee;
  font-size: 12px;
}

.tab-list {
  flex: 1;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.tab-list.active{
  color:#4189f4;
}
.tab-list image{
  width:50rpx;
  height:50rpx;
  margin-bottom:8rpx;
}