.tabbar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100rpx;
  border-top: 0.5px solid #EEEEEE;
  display: flex;
  font-size: 0;
  background: #fff;
  padding-bottom:10rpx;
}
.tabbar-item {
  flex: 1;
  text-align: center;
  overflow: hidden;
  box-sizing: border-box;
  padding: 8rpx 10rpx 0;
  font-size: 12px;
}
.tabbar-item-icon {
  margin-bottom: 6rpx;
  height: 50rpx;
}
.tabbar-item-icon image {
  width: 50rpx;
  height: 50rpx;
}
.tabbar-item-text {
  line-height: 20rpx;
  font-size:12px;
}
.tabbar-item.active {
  color: #4189F4;
}
.active .tabbar-item-text{
  color: #4189F4;
}