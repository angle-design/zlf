.lohgin_logo{
  margin-top:3vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
  width:100%;
  align-items: center;
  font-size:17px;
  margin-bottom:10vh;
}
.lohgin_logo image{
  width:750rpx;
  height:578rpx;
  margin-bottom:70rpx;
}
button{
  width:580rpx;
  height:80rpx;
  background:#06C15F;
  border-radius: 4px;
  color:#fff;
  font-size: 28rpx;
  line-height: 80rpx;
}
.agree{
  display: flex;
  width:100%;
  justify-content: center;
  align-items: center;
  font-size:24rpx;
  padding-top:50rpx;
  color:#666;
}
.agree image{
  width:26rpx;
  height:26rpx;
  margin-right:10rpx;
}
.agree view{
  display: flex;
}
.agree view view{
  color:#4189F4
}
.xieyi_pup{
  width:100%;
  height:100%;
  background:rgba(0,0,0,0.8);
  position: fixed;
  top:0;
  left:0;
}
.xieyi_con{
  width:630rpx;
  position:fixed;
  height:80vh;
  top:10vh;
  background: #fff;
  left:50%;
  margin-left:-315rpx;
  border-radius:20rpx;
  font-size:26rpx;
  padding:0 52rpx 80rpx;
  box-sizing: border-box;
  line-height:40rpx;
}
.tit{
  display: block;
  text-align: center;
  line-height:90rpx;
  font-size:32rpx;
  padding-top:20rpx;
}
.xieyi_con view{
  margin-top:20rpx;
}
.xieyi_con text.bold{
  display: block;
  font-weight: bold;
}
.close{
  width:64rpx;
  height:64rpx;
  position:absolute;
  top:0;
  right:-5rpx;
}
.foot_view {
  margin-top:20rpx;
}
.foot_view text{
  display: block;
  text-align: center;
}