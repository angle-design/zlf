page{
  width:100%;
  height:auto;
  background:url(https://satc.cscse.edu.cn/lxb-api/file/showImg/qGXApzda_20200716.jpg) no-repeat center top;
  background-size:750rpx 1414rpx;
  box-sizing: border-box;
}
.team_body{
  background:url(https://satc.cscse.edu.cn/lxb-api/file/showImg/RFqQ2Kc7_20200716.jpg) no-repeat center bottom;
  height:auto;
  background-size:750rpx 4626rpx;
  padding-bottom:30rpx;
}
.project{
margin-top:250rpx
}
.video{
  width:600rpx;
  height:340rpx;
  border-radius: 20rpx;
  overflow:hidden;
  margin:0 auto 30rpx;
  position: relative;

}
.video video{
  width:600rpx;
  height:340rpx;
  border-radius: 20rpx;
  overflow:hidden;
}
.video view{
  position: absolute;
  width:100rpx;
  height:100rpx;
  top:50%;
  margin-top:-50rpx;
  left:50%;
  margin-left:-50rpx
}
.video view image{
  width:100rpx;
  height:100rpx;
}
.project_one{
  width:674rpx;
  margin:100rpx auto 0;
  background:#fff;
  border-radius:10rpx;
  padding:90rpx 30rpx 30rpx;
  box-sizing: border-box;
  position: relative;
}
.project_one .title{
  width:334rpx;
  height:90rpx;
  background:url(https://satc.cscse.edu.cn/lxb-api/file/showImg/j7j6AQd0_20200716.png) no-repeat;
  background-size:334rpx 90rpx;
  position:absolute;
  top:-30rpx;
  left:0;
  font-size:42rpx;
  color:#fff;
  font-weight:bold;
  text-align: center;
  line-height:84rpx;
  padding-right:20rpx;
  box-sizing: border-box;
}
.project_one text{
  font-size:28rpx;
  color:#333;
  text-indent:2rem;
  display: block;
  line-height:50rpx;
}
.text_con{
  width:674rpx;
  font-size:28rpx;
  color:#fff;
  margin:40rpx auto;
  line-height:50rpx;
}
.text_con text{
  color:#ffc688
}